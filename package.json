{
  "name": "icoptimizer",
  "productName": "icoptimizer",
  "version": "0.2.7",
  "description": "icoptimizer for windows",
  "main": "main.js",
  "scripts": {
    "start": "node admin-launcher.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build": "electron-builder --win --x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "jqvon",
  "license": "GPL-3.0-only",
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "network-speed": "^2.1.1",
    "ping": "^0.4.4"
  },
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.jqvon.icoptimizer",
    "productName": "icoptimizer",
    "win": {
      "target": "nsis",
      "icon": "img/ic-icon.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "executableName": "icoptimizer"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true 
    },
    "extraFiles": [
      {
        "from": "debug/launch-debug.vbs",
        "to": "debug/launch-debug.vbs"
      }
    ]
  }
}
